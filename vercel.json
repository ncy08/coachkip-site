{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://kip.coach/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.kip.coach"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
